# platform :ios, '9.0'

pod 'AEXML'

target 'FruokIOiOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FruokIO

  target 'FruokIOiOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'FruokIOmacOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FruokIO macOS

  target 'FruokIOmacOSTests' do
    inherit! :search_paths
    # Pods for testing
  end
  
  target 'FruokIOmacOSUITests' do
      inherit! :search_paths
      # Pods for testing
  end

end
